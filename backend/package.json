{
  "name": "greencart-logistics-backend",
  "version": "1.0.0",
  "description": "GreenCart Logistics Backend API",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --watchAll --no-cache",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "seed": "node scripts/seedDatabase.js",
    "reset-db": "node scripts/resetDatabase.js",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write .",
    "docker:build": "docker build -t greencart-logistics-api .",
    "docker:run": "docker run -p 5000:5000 greencart-logistics-api"
  },
  "keywords": ["logistics", "delivery", "api", "express", "mongodb", "nodejs"],
  "author": "GreenCart Logistics Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-mongo-sanitize": "^2.2.0",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "dotenv": "^16.3.1",
    "compression": "^1.7.4",
    "express-slow-down": "^2.0.1",
    "hpp": "^0.2.3",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-node": "^4.1.0",
    "prettier": "^3.1.1",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
